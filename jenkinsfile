pipeline {
    agent any

    stages {
        stage('Cloning GitHub Repo to Jenkins') {
            steps {
                echo 'Cloning GitHub repo to Jenkins...'
                git branch: 'main',
                    url: 'https://github.com/yash8484/MLOPS_HOTEL_RESERVATION_PREDICTION.git',
                    credentialsId: 'github-token'
            }
        }
    }
}
